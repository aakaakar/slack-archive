                "fallback": "GitHub: ghaskins\/hyperledger-fabric-baseimage",
                "thumb_url": "https:\/\/avatars2.githubusercontent.com\/u\/367132?v=3&s=400",
                "from_url": "https:\/\/github.com\/ghaskins\/hyperledger-fabric-baseimage",
                "thumb_width": 400,

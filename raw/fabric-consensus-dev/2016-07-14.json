    },
    {
        "type": "message",
        "user": "U0XQ35CDD",
        "text": "`date: invalid date \u2018%s%9N\u2019`?",
        "ts": "1468504429.002160"
    },
    {
        "type": "message",
        "user": "U0XR6J961",

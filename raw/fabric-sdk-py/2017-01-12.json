    {
        "type": "message",
        "user": "U0Z41KY5V",
        "text": "Ok, any question?",
        "ts": "1484276958.000237"
    },
    {
        "type": "message",
        "user": "U0Z41KY5V",
        "text": "#3: MVP design discussion.",

        "ts": "1476414980.000118"
    },
    {
        "type": "message",
        "user": "U0Z41KY5V",
        "text": "Please go to this link and help review",
        "ts": "1476414991.000119"
    },
    {
        "type": "message",
        "user": "U1B7735PA",
        "text": "will this spec be the guide of all the sdk",
        "ts": "1476415028.000120"
    },
    {
        "type": "message",
        "user": "U1B7735PA",
        "text": "all the sdk projects?",
        "ts": "1476415033.000121"

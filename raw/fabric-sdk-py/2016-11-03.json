        "ts": "1478228973.000037"
    },
    {
        "type": "message",
        "user": "U0Z41KY5V",
        "text": "<@U0NCW1DPX>",
        "ts": "1478228978.000038"
    },
    {
        "type": "message",
        "user": "U0NCW1DPX",
